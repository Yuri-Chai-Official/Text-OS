            var input = Console.ReadLine();
            Console.Clear();
            Console.WriteLine("Welcome to TextOS Version 1H2");
            Console.WriteLine("**##**");
            Console.WriteLine("Use 'help' 'credits' 'version 'update'");
            Console.ReadLine();

            if (input == "update") ;
            {
                Console.WriteLine("Checking for updates...");
                await Task.Delay(4000);
                Console.WriteLine("You're up to date!");
            }

            if (input == "version") ;
            {
                Console.WriteLine("Software Information:");
                Console.WriteLine("OS Version: TextOS 1H2");
                Console.WriteLine("BIOS Version: Leaf Bios 0122");
                Console.WriteLine("Subsystem Slot #1: None Detected");
                Console.WriteLine("Subsystem Slot #2: None Detected");
                Console.WriteLine("Virtual Drive Slot #1: TextOS Installation Recovery");
            }

        }


        else
        {
            {
                Console.WriteLine("beans!");
            }
        }
    }

    else
    {
        Console.WriteLine("");
    }
}

else
{
    Console.WriteLine("Your BIOS is not up to date");
